[alias]
     s = status -s
     ss = status
     cm = commit -m
     diffc = diff --cached
     b = branch
     co = checkout
     ap = add -p
     reno = "!f() { git revert --no-commit HEAD~\"$1\".. ; }; f"
     t = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset'

[core]
     editor = "vim"

